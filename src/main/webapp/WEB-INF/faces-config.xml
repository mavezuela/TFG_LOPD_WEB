    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addversiones.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanVersion.add()}</from-action>
            <from-outcome>version</from-outcome>
