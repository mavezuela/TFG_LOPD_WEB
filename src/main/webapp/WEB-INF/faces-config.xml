    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanFicheros.add()}</from-action>
            <from-outcome>ficheros</from-outcome>
            <to-view-id>/vistas/ficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/vistas/editficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanFicheros.edit()}</from-action>
            <from-outcome>ficheros</from-outcome>
            <to-view-id>/vistas/ficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!--Gestion Ficheros-->   
    <navigation-rule>
        <from-view-id>/vistas/gestionficheros.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addgestficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>       
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editgestficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/vistas/gestiondepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>                        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addgestficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanFicheros.addgest()}</from-action>
            <from-outcome>ficherosgest</from-outcome>
            <to-view-id>/vistas/gestionficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/editgestficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanFicheros.editgest()}</from-action>
            <from-outcome>ficherosgest</from-outcome>
            <to-view-id>/vistas/gestionficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>   
    
    <!--Version-->   
    <navigation-rule>
        <from-view-id>/vistas/versiones.xhtml</from-view-id>
