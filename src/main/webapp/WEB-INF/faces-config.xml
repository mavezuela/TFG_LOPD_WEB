<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>     
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.loginUsuario}</from-action>
            <from-outcome>Administrador</from-outcome>
            <to-view-id>/vistas/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.loginUsuario}</from-action>
            <from-outcome>Operador</from-outcome>
            <to-view-id>/vistas/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.loginUsuario}</from-action>
            <from-outcome>Consultor</from-outcome>
            <to-view-id>/vistas/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/home.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.logoutUsuario}</from-action>
            <from-outcome>Salir</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/usuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addusuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editusuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addusuario.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanUsuarios.add}</from-action>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/vistas/usuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/editusuario.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanUsuarios.edit()}</from-action>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/vistas/usuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
<!--Entidades-->   
