<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>     
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.loginUsuario}</from-action>
            <from-outcome>Administrador</from-outcome>
            <to-view-id>/vistas/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.loginUsuario}</from-action>
            <from-outcome>Operador</from-outcome>
            <to-view-id>/vistas/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.loginUsuario}</from-action>
            <from-outcome>Consultor</from-outcome>
            <to-view-id>/vistas/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/home.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanLoginUsuarios.logoutUsuario}</from-action>
            <from-outcome>Salir</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/usuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addusuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editusuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addusuario.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanUsuarios.add}</from-action>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/vistas/usuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/editusuario.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanUsuarios.edit()}</from-action>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/vistas/usuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!--Entidades-->   
    <navigation-rule>
        <from-view-id>/vistas/entidades.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addentidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editentidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case>        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addentidades.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanEntidad.add()}</from-action>
            <from-outcome>entidad</from-outcome>
            <to-view-id>/vistas/entidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/vistas/editentidades.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanEntidad.edit()}</from-action>
            <from-outcome>entidad</from-outcome>
            <to-view-id>/vistas/entidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!--Planificar Proyectos-->
    <navigation-rule>
        <from-view-id>/vistas/planifproyecto.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanEntidad.planificar()}</from-action>            
            <from-outcome>departamentogest</from-outcome>
            <to-view-id>/vistas/gestiondepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>               
    </navigation-rule>       
     
    <!--Gestion Entidades-->   
    <navigation-rule>
        <from-view-id>/vistas/gestionentidades.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editgestentidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>departamentos</from-outcome>
            <to-view-id>/vistas/gestiondepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>               
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/editgestentidades.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanEntidad.editgest()}</from-action>
            <from-outcome>entidadgest</from-outcome>
            <to-view-id>/vistas/gestionentidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
  
    <!--Departamento-->   
    <navigation-rule>
        <from-view-id>/vistas/departamento.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editdepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>         
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/adddepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/adddepartamento.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanDepartamento.add()}</from-action>
            <from-outcome>departamento</from-outcome>
            <to-view-id>/vistas/departamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/vistas/editdepartamento.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanDepartamento.edit()}</from-action>
            <from-outcome>departamento</from-outcome>
            <to-view-id>/vistas/departamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!--Gestion Departamento-->   
    <navigation-rule>
        <from-view-id>/vistas/gestiondepartamento.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addgestdepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>       
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editgestdepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>ficheros</from-outcome>
            <to-view-id>/vistas/gestionficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/vistas/gestionentidades.xhtml</to-view-id>
            <redirect/>
        </navigation-case>                        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addgestdepartamento.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanDepartamento.addgest()}</from-action>
            <from-outcome>departamentogest</from-outcome>
            <to-view-id>/vistas/gestiondepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/vistas/editgestdepartamento.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanDepartamento.editgest()}</from-action>
            <from-outcome>departamentogest</from-outcome>
            <to-view-id>/vistas/gestiondepartamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>       
    
    <!--Ficheros-->   
    <navigation-rule>
        <from-view-id>/vistas/ficheros.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editficheros.xhtml</to-view-id>
            <redirect/>
