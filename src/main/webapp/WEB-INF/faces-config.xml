        </navigation-case>              
    </navigation-rule>
    <!--Recoger Categorias-->
    <navigation-rule>
        <from-view-id>/vistas/recogidapreg.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/vistas/recogidacat.xhtml</to-view-id>
            <redirect/>
        </navigation-case>              
    </navigation-rule>                 
    <!--Managed beans-->   
    <managed-bean>
        <managed-bean-name>managedBeanLoginUsuarios</managed-bean-name>
        <managed-bean-class>managedbeans.ManagedBeanLoginUsuarios</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>managedBeanUsuarios</managed-bean-name>
        <managed-bean-class>managedbeans.ManagedBeanUsuarios</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
