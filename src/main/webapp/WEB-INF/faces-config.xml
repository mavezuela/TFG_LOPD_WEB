    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/vistas/editversiones.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanVersion.edit()}</from-action>
            <from-outcome>version</from-outcome>
            <to-view-id>/vistas/versiones.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!--Versionfichero-->   
    <navigation-rule>
        <from-view-id>/vistas/mantficheros.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/vistas/addmantficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/vistas/editmantficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/addmantficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanVersionFichero.add()}</from-action>
            <from-outcome>versionfichero</from-outcome>
            <to-view-id>/vistas/mantficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/vistas/editmantficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanVersionFichero.edit()}</from-action>
            <from-outcome>versionfichero</from-outcome>
            <to-view-id>/vistas/mantficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <!--estficheros-->
    <navigation-rule>
        <from-view-id>/vistas/estficheros.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{managedBeanCatpreguntas.cumplimiento()}</from-action>           
            <from-outcome>estadisticafic</from-outcome>
            <to-view-id>/vistas/estadisticafic.xhtml</to-view-id>
            <redirect/>
        </navigation-case>      
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/vistas/estadisticafic.xhtml</from-view-id>
        <navigation-case>    
            <from-outcome>volver</from-outcome>
            <to-view-id>/vistas/estficheros.xhtml</to-view-id>
            <redirect/>
        </navigation-case>      
    </navigation-rule>
    <!--Recoger Datos-->
    <navigation-rule>
        <from-view-id>/vistas/recogidadatos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>categorias</from-outcome>
            <to-view-id>/vistas/recogidacat.xhtml</to-view-id>
            <redirect/>
        </navigation-case>      
    </navigation-rule>
    <!--Recoger Categorias-->
    <navigation-rule>
        <from-view-id>/vistas/recogidacat.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>preguntas</from-outcome>
            <to-view-id>/vistas/recogidapreg.xhtml</to-view-id>
